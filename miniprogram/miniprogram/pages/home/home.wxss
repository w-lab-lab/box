/* miniprogram/pages/home/home.wxss */
.homePage {
  background-color: #fed763;
}

.UCenter-bg {
  background-image: url(https://7778-wxapp1-2gyuivu17f31ebac-1305836354.tcb.qcloud.la/cloudpic/homepic.png?sign=cd0d5c9b10f1706e08885beafbaca382&t=1620655689);
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.flexbutton {
  width: 100%;
  height: 500rpx;
}

.flexbutton2 {
  width: 100%;
  height: 210rpx;
}

.homeimages {
  height: 80px;
}

.homeimages2 {
  width: 100%;
  height: 70px;
}

.homebutton {
  width: 100%;
  height: 70px;
}

.null {
  width: 100%;
  height: 150px;
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}


.bar-wrapper{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}

.hline{
  margin-bottom: 120rpx;
}